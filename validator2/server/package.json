    "@types/node": "^12.7.7",
    "@types/node-fetch": "^2.5.7",
    "@types/request": "^2.48.3",
    "@types/request-promise-native": "^1.0.17",
    "@types/rimraf": "^3.0.0",
    "@types/sinon": "^7.0.13",
    "@types/stack-trace": "^0.0.29",
    "@types/supertest": "^2.0.9",
    "@typescript-eslint/eslint-plugin": "^2.9.0",
    "@typescript-eslint/parser": "^2.9.0",
    "chai": "^4.2.0",
    "cookie": "^0.4.1",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.1.0",
