    "@types/compression": "^1.0.1",
    "@types/cookie": "^0.3.3",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.6",
    "@types/errorhandler": "^1.5.0",
    "@types/express": "^4.17.1",
    "@types/express-session": "^1.17.0",
    "@types/http-errors": "^1.6.2",
    "@types/json-stringify-safe": "^5.0.0",
    "@types/lowdb": "^1.0.9",
    "@types/memoizee": "^0.4.4",
    "@types/mocha": "^7.0.2",
