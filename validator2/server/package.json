{
  "name": "dappnode-medalla-validator-api-server",
  "version": "1.0.0",
  "description": "Validator dashboard API server",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "build": "webpack",
    "mock": "nodemon -w . -x NODE_ENV=development DISABLE_PASSWORD=true ts-node mock",
    "dev": "nodemon",
    "dev:file": "ts-node -r dotenv/config",
    "test": "npm run test:file \"./{,!(node_modules)/**}/*.test.ts\" ",
    "test:file": "mocha -r dotenv/config -r ts-node/register --exit ",
    "test:int": "npm run test:int:file \"./{,!(node_modules)/**}/*.test.int.ts\" ",
    "test:int:file": "mocha -r dotenv/config -r ts-node/register --exit",
    "pre-commit": "npm run lint && npm run test",
    "analyze": "webpack --profile --json > build/stats.json && npx webpack-bundle-analyzer build/stats.json",
    "common-build": "node common/validation/generateSchemas",
    "common-copy": "rm -rf ../ui/src/common/ && cp -r common/ ../ui/src/common/",
    "common-watch": "node scripts/sync-common",
    "prepare": "patch-package",
    "lint": "eslint . --ext .ts --fix",
    "prettier": "prettier --write 'src/**/*.*' 'test/**/*.*'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dappnode/DAppNodePackage-prysm-validator.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dappnode/DAppNodePackage-prysm-validator/issues"
  },
  "homepage": "https://github.com/dappnode/DAppNodePackage-prysm-validator#readme",
  "dependencies": {
    "adm-zip": "^0.4.16",
    "ajv": "^6.12.2",
    "async-retry": "^1.3.1",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dargs": "^7.0.0",
    "ethers": "^4.0.47",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "json-stringify-safe": "^5.0.1",
    "lodash": "^4.17.15",
    "lowdb": "^1.0.0",
    "memoizee": "^0.4.14",
    "morgan": "^1.9.1",
    "node-fetch": "^2.6.0",
    "rimraf": "^3.0.2",
    "session-file-store": "^1.4.0",
    "source-map-support": "^0.5.13",
    "stack-trace": "^0.0.10"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.4.33",
    "@types/assert": "^1.4.6",
